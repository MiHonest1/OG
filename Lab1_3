#include <iostream>
#include <ctime>
#include <malloc.h>
using namespace std;

int n, * p = NULL;

int main() {

setlocale(LC_ALL, "Russian");

cout << "\nВведите размерность массива: ";
cin >> n;
cout << "Массив: ";
p = (int*)calloc(n, 2);
   
for (int i = 0; i < n; i++) {
 *(p + i) = 1 + rand() % 100;
 cout << *(p + i) << " ";
}

return 0;
}
